<div class=''>
  <% if @current_user.profile_image? %>
    <%= image_tag @current_user.profile_image %>
  <% else %>
    <%= image_tag 'https://www.goaltos.com/wp-content/uploads/sites/4559/2018/01/avatar-1577909_960_720.png' %>
  <% end %>
  <% if @current_user.present? %>
    <h4 class=''><%= @current_user.user_name %></h4>
  <% end %>
</div>
